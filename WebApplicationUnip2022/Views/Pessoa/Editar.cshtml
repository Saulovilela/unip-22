@{
    ViewData["Title"] = "Editar Contato";
}
<div class="text-left">
    <h1 class="display-4">Editar Contato</h1>

</div>
<br />
<h3>Dados Pessoais</h3>

<form>
    <div class="form-group">
        <label for="Nome">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o Nome">
    </div>
    <div class="form-group">
        <label for="Nome">CPF</label>
        <input type="text" class="form-control" placeholder="Digite o CPF">
    </div>
</form>
<br />
<h3>Endereço</h3>
<form>
    <div class="form-group">
        <label for="Nome">Cidade</label>
        <input type="text" class="form-control" placeholder="Digite a Cidade">
    </div>
    <div class="form-group">
        <label for="Nome">Estado</label>
        <input type="text" class="form-control" placeholder="Digite o Estado">
    </div>
    <div class="form-group">
        <label for="Nome">CEP</label>
        <input type="text" class="form-control" placeholder="Digite o CEP">
    </div>
</form>
<h3>Telefone</h3>
<form>
    <div class="form-group">
        <label for="Nome"></label>
        <input type="text" class="form-control" placeholder="Digite o telefone">
    </div>
    <h4>tipo de telefone</h4>
    <div class="form-check">
        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios1" value="option1" checked>
        <label class="form-check-label" for="exampleRadios1">
            Telefone Fixo
        </label>
    </div>
    <div class="form-check">
        <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios2" value="option2">
        <label class="form-check-label" for="exampleRadios2">
            Celular
        </label>
    </div>
</form>